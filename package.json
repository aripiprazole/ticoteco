  "license": "GPL-3.0+",
  "packageManager": "yarn@3.2.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "heroku-postbuild": "yarn workspace @ticoteco/backend build && yarn patch-package",
    "backend:dev": "yarn workspace @ticoteco/backend dev",
    "backend:dev:watch": "yarn workspace @ticoteco/backend dev:watch",
    "backend:start": "yarn workspace @ticoteco/backend start",
    "backend:build": "yarn workspace @ticoteco/backend build",
    "backend:schema": "yarn workspace @ticoteco/backend schema",
